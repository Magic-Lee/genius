<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.genius.study.systemUserinfo.mapper.SystemUserinfoMapper">
    <select id="queryListBySex" resultType="cn.genius.study.entity.SystemUserinfo">
        SELECT * FROM SYSTEM_USERINFO T WHERE T.SEX = #{sex,jdbcType=VARCHAR}
    </select>
</mapper>
